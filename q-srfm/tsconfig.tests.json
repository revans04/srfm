{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "skipLibCheck": true,
    "outDir": "dist-tests"
  },
  "include": [
    "src/env.d.ts",
    "src/composables/**/*.ts",
    "src/utils/**/*.ts",
    "src/store/**/*.ts",
    "src/types.ts",
    "tests/**/*.ts"
  ]
  ,"exclude": [
    "src/store/index.ts"
  ]
}
